{
  "name": "typescript-react-apollo",
  "version": "0.0.1",
  "description": "Boilerplate for graphql application with TypeScript and React",
  "main": "dist/index.js",
  "scripts": {
    "dev-server": "ts-node src/server/index.ts -p tsconfig.server.json",
    "dev-client": "webpack-dev-server --open --mode development",
    "generate-schema": "graphql-codegen --config codegen.yaml",
    "migrate-generate": "ts-node ./node_modules/.bin/typeorm migration:generate -n Initialize",
    "migrate-up": "ts-node ./node_modules/.bin/typeorm migration:run",
    "migrate-down": "ts-node ./node_modules/.bin/typeorm migration:revert",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format": "prettier --write --ignore-path .eslintignore 'src/**/*.{js,jsx,ts,tsx}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KanchiShimono/typescript-react-apollo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KanchiShimono/typescript-react-apollo/issues"
  },
  "homepage": "https://github.com/KanchiShimono/typescript-react-apollo#readme",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.16.0",
    "@graphql-codegen/introspection": "^1.16.0",
    "@graphql-codegen/typescript": "^1.16.0",
    "@graphql-codegen/typescript-operations": "^1.16.0",
    "@graphql-codegen/typescript-react-apollo": "^1.16.0",
    "@graphql-codegen/typescript-resolvers": "^1.16.0",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "@typescript-eslint/typescript-estree": "^4.14.1",
    "eslint": "^7.3.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.5",
    "prettier": "^2.0.5",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.6",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.5",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "apollo-boost": "^0.4.9",
    "apollo-server": "^2.15.1",
    "graphql": "^15.2.0",
    "graphql-tag": "^2.10.3",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "react-dom": "^16.13.1",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "ts-node": "^8.10.2",
    "typeorm": "^0.2.25"
  }
}
